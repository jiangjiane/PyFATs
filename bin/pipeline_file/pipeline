5ttgen fsl {subjects_path_id}/T1w_acpc_dc_restore_brain1.25.nii.gz {subjects_path_id}/Diffusion/preproc/5TT.mif -premasked -force
mrconvert {subjects_path_id}/Diffusion/data.nii.gz {subjects_path_id}/Diffusion/preproc/DWI.mif -fslgrad {subjects_path_id}/Diffusion/bvecs {subjects_path_id}/Diffusion/bvals -datatype float32 -stride 0,0,0,1 -force
dwiextract {subjects_path_id}/Diffusion/preproc/DWI.mif - -bzero | mrmath - mean {subjects_path_id}/Diffusion/preproc/mean0.mif -axis 3 -force
dwi2response dhollander {subjects_path_id}/Diffusion/preproc/DWI.mif {subjects_path_id}/Diffusion/preproc/RF_WM.txt {subjects_path_id}/Diffusion/preproc/RF_GM.txt {subjects_path_id}/Diffusion/preproc/RF_CSF.txt -voxels {subjects_path_id}/Diffusion/preproc/RF_voxels.mif -force
dwi2fod msmt_csd {subjects_path_id}/Diffusion/preproc/DWI.mif {subjects_path_id}/Diffusion/preproc/RF_WM.txt {subjects_path_id}/Diffusion/preproc/WM_FODs.mif {subjects_path_id}/Diffusion/preproc/RF_GM.txt {subjects_path_id}/Diffusion/preproc/GM.mif {subjects_path_id}/Diffusion/preproc/RF_CSF.txt {subjects_path_id}/Diffusion/preproc/CSF.mif -mask {subjects_path_id}/Diffusion/nodif_brain_mask.nii.gz -force
tckgen {subjects_path_id}/Diffusion/preproc/WM_FODs.mif {subjects_path_id}/Diffusion/tractography/Det/SD_Stream_angle20_cutoff0.1_length50_250_seedFP_roiOccipital_L_100k.tck -algorithm SD_Stream -act {subjects_path_id}/Diffusion/preproc/5TT.mif -crop_at_gmwmi -include {subjects_path_id}/ROI/L_Occipital.nii.gz -seed_image {subjects_path_id}/ROI/FP_prob.nii.gz -angle 20 -minlength 50 -maxlength 250 -select 100k -cutoff 0.1 -force
tckgen {subjects_path_id}/Diffusion/preproc/WM_FODs.mif {subjects_path_id}/Diffusion/tractography/Det/SD_Stream_angle20_cutoff0.1_length50_250_seedFP_roiOccipital_R_100k.tck -algorithm SD_Stream -act {subjects_path_id}/Diffusion/preproc/5TT.mif -crop_at_gmwmi -include {subjects_path_id}/ROI/R_Occipital.nii.gz -seed_image {subjects_path_id}/ROI/FP_prob.nii.gz -angle 20 -minlength 50 -maxlength 250 -select 100k -cutoff 0.1 -force