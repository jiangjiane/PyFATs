dwi2tensor {subjects_path_id}/Diffusion/data/data.nii.gz -fslgrad {subjects_path_id}/Diffusion/data/bvecs {subjects_path_id}/Diffusion/data/bvals - | tensor2metric - -adc {destination_path}/adc.mif -fa {destination_path}/fa.mif -ad {destination_path}/ad.mif -rd {destination_path}/rd.mif -cl {destination_path}/cl.mif -cp {destination_path}/cp.mif -cs {destination_path}/cs.mif -value {destination_path}/value.mif -vector {destination_path}/vector.mif -mask {subjects_path_id}/Diffusion/data/nodif_brain_mask.nii.gz