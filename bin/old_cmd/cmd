dwi2tensor %s/Diffusion/data/data.nii.gz -fslgrad %s/Diffusion/data/bvecs %s/Diffusion/data/bvals - | tensor2metric - -adc %s/Diffusion/metrics/adc.mif -fa %s/Diffusion/metrics/fa.mif -ad %s/Diffusion/metrics/ad.mif -rd %s/Diffusion/metrics/rd.mif -cl %s/Diffusion/metrics/cl.mif -cp %s/Diffusion/metrics/cp.mif -cs %s/Diffusion/metrics/cs.mif -value %s/Diffusion/metrics/value.mif -vector %s/Diffusion/metrics/vector.mif -mask %s/Diffusion/data/nodif_brain_mask.nii.gz
